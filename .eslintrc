{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
    "react/forbid-prop-types": "off",
    "no-param-reassign": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "prefer-destructuring": "warn",
    "react/jsx-filename-extension": "off",
    "jsx-a11y/click-events-have-key-events": "off",
    "linebreak-style": 0
  }
}
